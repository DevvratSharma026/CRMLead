# MongoDB Connection String
# Get this from MongoDB Atlas: https://cloud.mongodb.com
# Format: mongodb+srv://<username>:<password>@<cluster-url>/<database-name>?retryWrites=true&w=majority
MONGO_URI=mongodb+srv://username:password@cluster0.xxxxx.mongodb.net/leads_db?retryWrites=true&w=majority

# Server Port
PORT=5000

# Node Environment
NODE_ENV=development
